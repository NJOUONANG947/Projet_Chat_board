generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data/database.db"
}

model Message {
  id        Int      @id @default(autoincrement())
  content   String
  role      String   // 'user' or 'assistant'
  createdAt DateTime @default(now())
}
